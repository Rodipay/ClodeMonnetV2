<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:ClodeMonnetV2.View"
                    xmlns:vm="clr-namespace:ClodeMonnetV2.ViewModel">

    <DataTemplate DataType="{x:Type vm:WaiterVM}">
        <view:WaiterView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CookVM}">
        <view:CookView></view:CookView>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ChefVM}">
        <view:ChefView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ManagerVM}">
        <view:ManagerView />
    </DataTemplate>

</ResourceDictionary>